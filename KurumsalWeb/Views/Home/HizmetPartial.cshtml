@model IEnumerable<KurumsalWeb.Models.Model.Hizmetler>

<h4 class="heading">Yaptığımız Projeler</h4>
<div class="row">
    <section id="projects">
        <ul id="thumbs" class="portfolio">
            <!-- Item Project and Filter Name -->
            @foreach (var x in Model)
            {
                <li class="col-lg-3 design" data-id="id-0" data-type="web">
                    <div class="item-thumbs">
                        <!-- Fancybox - Gallery Enabled - Title - Full Image -->
                        <a class="hover-wrap fancybox" data-fancybox-group="gallery" title="@x.Baslik" href="@x.ResimURL">
                            <span class="overlay-img"></span>
                            <span class="overlay-img-thumb font-icon-plus"></span>
                        </a>
                        <!-- Thumb Image and Description -->
                        <img src="@x.ResimURL" alt="@x.Aciklama">
                    </div>
                </li>
            }
        </ul>
    </section>
</div>
